void read(int*);
void write(int);

int i ;

void print (int x )
{
	write (x);
	return 0;
}

int inc (int x )
{
	return x + 1;
}

int sqr (int x )
{
	return x * x;
}

void main()
{
	read (&i);
	while (i)
	{
		print (i);
		print (inc (sqr (inc (i))));
		read (&i);
	}
}
