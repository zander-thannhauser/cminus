
void printf(const char*, ...);

static struct {
	float f;
	struct { int i, j; } subs[4];
} a = {3.14, {0, 0}, {1, 0}, {0, 1} },
  b; // should be initialized with zeros

int main ()
{
	struct c {
		float f1, f2, f3;
	} c = {1, 2, 3}; // should expand to multiple assignments
	
	b.f = a.f + c.f2;
	b.subs[2] = a.subs[0];
	
	c = (struct c) {c.f3, c.f2, c.f1};
	
	b.f += c.f1;
	
	printf("b.f == %lf\n", b.f);
	printf("b.subs[2] == {%i, %i}\n", b.subs[2].i, b.subs[2].j);
	
	return 0;
}





