
void printf(const char*, ...);
void exit(int);

void main ()
{
	int i, j, k, l;
	
	i = 1; j = 0; k = 0; l = 1;
	
	printf("i && j == %s\n", i && j ? "true" : "false"); // false
	printf("i && i == %s\n", i && i ? "true" : "false"); // true
	printf("k && l == %s\n", k && l ? "true" : "false"); // false
	printf("k && j == %s\n", k && j ? "true" : "false"); // false
	
	exit(0);
}
