
void printf(const char*, ...);
void exit(int);

void main ()
{
	float i, j, k, l;
	
	i = 1.3; j = 0.44; k = 0.75; l = 1.99;
	
	printf("i && j == %s\n", i && j ? "true" : "false");
	printf("i && i == %s\n", i && i ? "true" : "false");
	printf("k && l == %s\n", k && l ? "true" : "false");
	printf("k && j == %s\n", k && j ? "true" : "false");
	
	exit(0);
}
