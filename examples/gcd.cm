
void read(int*), write(int), exit();

int x, y;

int gcd(int x, int y) {
      int t;
         if (y==0) {
            return x;
	 }
         else {
	      t = x;
	      x = y;
	      y = t - y * (t / y);
	      return gcd(x,y);
	 }
}

void main() {
         read (&x);
         read (&y);
         while ((x != 0) || (y != 0)) {
	       write (gcd(x,y));
               read (&x);
               read (&y);
         }
	exit();
}
