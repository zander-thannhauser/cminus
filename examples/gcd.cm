
#ifdef X64_TARGET
#else
void iread(int*), iwrite(int);
#endif

int gcd(int x, int y) {
	int t;
	
	if (y == 0) {
		return x;
	} else {
		t = x;
		x = y;
		y = t - y * (t / y);
		return gcd(x, y);
	}
}

void main() {
	int x, y;
	
	iread (&x);
	iread (&y);
	
	while ((x != 0) || (y != 0)) {
		iwrite (gcd(x, y));
		iread (&x);
		iread (&y);
	}
	
	#ifdef X64_TARGET
	exit();
	#endif
}
